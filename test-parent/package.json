{
  "name": "@dodo/test-parent",
  "version": "1.0.0",
  "description": "",
  "main": "./lib",
  "scripts": {
    "eslint": "eslint src --ext jsx,js",
    "eslint-fix": "eslint src --ext jsx,js --fix",
    "clean": "rm -rf ./lib && mkdir ./lib",
    "build": "npm run clean && npm run eslint && ./.scripts/build.sh",
    "prepublish": "npm run test",
    "test": "npm run build && ./.scripts/test.sh",
    "test-tap":
      "npm run build && ./.scripts/test.sh --reporter mocha-tap-reporter ./test/index.js",
    "preversion": "npm run test",
    "postversion": "git push origin master && git push --tags"
  },
  "publishConfig": {
    "registry": "https://nexus.nemeabank.com/repository/npm-local/"
  },
  "author": "",
  "dependencies": {
    "lodash.omit": "^4.1.1",
    "babel-runtime": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-register": "^6.6.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "mocha-tap-reporter": "^0.1.3",
    "babel-eslint": "^6.0.0",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.1.0"
  }
}
