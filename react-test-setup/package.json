  },
  "dependencies": {
    "babel-runtime": "^6.6.0"
  },
  "devDependencies": {
    "@nemea/eslint-config-es6": "~0.1.0",
    "@nemea/test-parent": "^1.0.0",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.1.0",
    "jsdom": "^8.1.0",
