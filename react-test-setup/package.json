    "react-dom": "^15.0.0",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "@nemea/test-parent": "^1.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.1.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "react": "^15.0.0"
  }
}
