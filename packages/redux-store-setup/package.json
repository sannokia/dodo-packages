  "author": "Adrian Bonnici <adrian.bonnici@isobar.com>",
  "main": "./dist/index.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0"
  },
  "peerDependencies": {
    "eslint": "^4.16.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@dodo/eslint-config-base": "^0.1.1",
    "@dodo/eslint-config-prettier": "^0.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
